entrypoint = "run.command"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json", "run.command"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"
packages = ["opensshWithKerberos"]

[deployment]
run = ["node", "server.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[run]
run = "bash run.command"
alwaysRun = true

[[ports]]
localPort = 3000
externalPort = 80
